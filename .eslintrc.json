{
  "extends": "react-app",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": ["warn", "global"]
  },
  "overrides": [
    {
      "files": ["test/fixtures/*/src/**"],
      "parserOptions": {
        "sourceType": "module"
      },
      "rules": {
        "strict": ["warn", "never"]
      }
    }
  ]
}
